.grid-group{
  border-top:1rpx solid #d9d9d9;
  border-left:1rpx solid #d9d9d9;
  overflow:hidden
}
.grid{
  position:relative;
  float:left;
  padding:20px 10px;
  width:33.33333333%;
  box-sizing:border-box;
  border-right:1rpx solid #d9d9d9;
  border-bottom:1rpx solid #d9d9d9
}
.grid_active{
  background-color:#ececec
}
.grid_icon{
  display:block;
  width:28px;
  height:28px;
  margin:0 auto
}
.grid_label{
  margin-top:5px;
  display:block;
  text-align:center;
  color:#000;
  font-size:14px;
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden}